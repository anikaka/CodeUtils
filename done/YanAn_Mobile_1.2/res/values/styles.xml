<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="login_save_password_checkbox_style" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/selector_checkbox_login</item>
    </style>

    <style name="layout_title_style">
        <item name="android:layout_height">@dimen/rlHeight_layout_title</item>
        <item name="android:background">@color/main_top</item>
        <item name="android:textSize">@dimen/textSize_layout_title</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="txtTitle_layout_style">
        <item name="android:textSize">@dimen/textSize_layout_title</item>
        <item name="android:textColor">@color/white</item>
    </style>
	
    
    <style name="txtContent_layout_subside_pactselect_data">
        <item name="android:textColor">@color/txtColor_layout_subside_paceselect</item>
        <item name="android:textSize">@dimen/txtColor_layout_subside_paceselect</item>
    </style>

    <style name="txtRow_common_layout_sudsie_pactselect_data_style">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/txtSize_layout_subside_pactselect_data</item>
    </style>
    
    <style  	 name="txtRow_pic">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize" >@dimen/common_row_size_content</item>
    </style>
       <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>边框
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item>半透明
        <item name="android:windowNoTitle">true</item><!--无标题-->
        <item  name="android:windowBackground">@color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>模糊
    </style>
    <style  	 name="txtRow_quality">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize" >@dimen/common_row_size_content</item>
        <item name="android:singleLine">true</item>
      </style>
      
</resources>