<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:id="@+id/rlTitle_layout_subside_pactselect_data_modify_data_look"
        style="@style/layout_title_style"
        android:layout_width="match_parent" >

        <TextView
            android:id="@+id/txtTile_layout_subside_pactselect_data_modify_data_look"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/txtTitle_data"
            android:textColor="@color/white"
            android:textSize="@dimen/txtSize_data_add_or_modify_title" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/icon_home_normal" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rlContent_layout_subside_pactselect_data_modify_supervise_data_modify"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/rlTitle_layout_subside_pactselect_data_modify_data_look"
        android:background="@color/light_gray"
        android:scrollbars="vertical" >

        <RelativeLayout
            android:id="@+id/rlLine_layout_subside_pactselect_data_modify_supervise_data_modify"
            android:layout_width="match_parent"
            android:layout_height="@dimen/rlHeight_layout_subside_pactselect_data_look"
            android:background="@drawable/common_bgcolor_title" >
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rlType_layout_subside_pactselect_data_modify_supervise_data_values"
            android:layout_width="match_parent"
            android:layout_height="@dimen/rlHeight_layout_subside_pactselect_data_row"
            android:layout_below="@id/rlLine_layout_subside_pactselect_data_modify_supervise_data_modify"
            android:background="@drawable/selector_common_row" >

            <TextView
                android:id="@+id/txtSupervise_layout_subside_pactselect_data_modify_type_title_modify"
                style="@style/txtRow_common_layout_sudsie_pactselect_data_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="@string/txtValue_Supervisetype_title" />

            <TextView
                android:id="@+id/txtSupervise_layout_subside_pactselect_data_modifys_type_title_content"
                style="@style/txtRow_common_layout_sudsie_pactselect_data_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/txtSupervise_layout_subside_pactselect_data_modify_type_title_modify"
                android:text="@string/txtValue_Supervisetype_content" />
        </RelativeLayout>

        <View
            android:id="@+id/View_data_values"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_below="@id/rlType_layout_subside_pactselect_data_modify_supervise_data_values"
            android:background="@color/gray" />

        <RelativeLayout
            android:id="@+id/rlSupervise_layout_subside_pactselect_data_modify_project"
            android:layout_width="match_parent"
            android:layout_height="@dimen/rlHeight_layout_subside_pactselect_data_row"
            android:layout_below="@id/View_data_values"
            android:background="@drawable/selector_common_row" >

            <TextView
                android:id="@+id/txtSupervise_layout_subside_pactselect_data_modify_title"
                style="@style/txtRow_common_layout_sudsie_pactselect_data_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="@string/txtValue_SuperviseProgram_title" />

            <TextView
                android:id="@+id/txtSupervise_layout_subside_pactselect_data_modify_content"
                style="@style/txtRow_common_layout_sudsie_pactselect_data_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/txtSupervise_layout_subside_pactselect_data_modify_title"
                android:text="@string/txtValue_SupervisePragram_content" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rlLine_layout_subside_pactselect_data_modify_change_values"
            android:layout_width="match_parent"
            android:layout_height="@dimen/rlHeight_layout_subside_pactselect_data_look"
            android:layout_below="@id/rlSupervise_layout_subside_pactselect_data_modify_project"
            android:background="@drawable/common_bgcolor_title" >
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rlMonitorValue_layout_subside_pactselect_data_modify"
            android:layout_width="match_parent"
            android:layout_height="@dimen/rlHeight_layout_subside_pactselect_data_row"
            android:layout_below="@id/rlLine_layout_subside_pactselect_data_modify_change_values"
            android:background="@color/light_gray" >

            <TextView
                android:id="@+id/txtMonitorValue_layout_subside_pactselect_data_modify_change_values"
                style="@style/txtRow_common_layout_sudsie_pactselect_data_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="@string/txtMonitorValue_data_add_or_modify_title" />

            <EditText
                android:id="@+id/editMonitorValue_layout_subside_pactselect_data_modify_change_values"
                android:layout_width="match_parent"
                android:layout_height="@dimen/common_EditViewHeight"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="50dp"
                android:layout_toRightOf="@id/txtMonitorValue_layout_subside_pactselect_data_modify_change_values"
                android:background="@color/light_gray"
                android:digits="0123456789"
                android:windowSoftInputMode="adjustUnspecified|stateHidden" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="20dp"
                android:focusable="false"
                android:src="@drawable/icon_right_arrows_normal" />
        </RelativeLayout>

        <View
            android:id="@+id/View_change_values"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_below="@id/rlMonitorValue_layout_subside_pactselect_data_modify"
            android:background="@color/gray" />

        <RelativeLayout
            android:id="@+id/rlMonitorDeep_layout_subside_pactselect_data_modify"
            android:layout_width="match_parent"
            android:layout_height="@dimen/rlHeight_layout_subside_pactselect_data_row"
            android:layout_below="@id/View_change_values"
            android:background="@color/light_gray" >

            <TextView
                android:id="@+id/txtMonitorDeep_layout_subside_pactselect_data_modify_change_values"
                style="@style/txtRow_common_layout_sudsie_pactselect_data_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="@string/txtMonitorDeep_data_add_or_modify_title" />

            <EditText
                android:id="@+id/editMonitorDeep_layout_subside_pactselect_data_modify_change_values"
                android:layout_width="match_parent"
                android:layout_height="@dimen/common_EditViewHeight"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="50dp"
                android:layout_toRightOf="@id/txtMonitorDeep_layout_subside_pactselect_data_modify_change_values"
                android:background="@color/light_gray"
                android:digits="0123456789"
                android:windowSoftInputMode="adjustUnspecified|stateHidden" />

            
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="20dp"
                android:focusable="false"
                android:src="@drawable/icon_right_arrows_normal" />
        </RelativeLayout>
                <View
            android:id="@+id/View_Monitordeep_values"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_below="@id/rlMonitorDeep_layout_subside_pactselect_data_modify"
            android:background="@color/gray" />
        <RelativeLayout
            android:id="@+id/rlValueDataUnit_title_layout_subside_pactselect_data_modify"
            android:layout_width="match_parent"
            android:layout_height="@dimen/rlHeight_layout_subside_pactselect_data_row"
            android:layout_below="@id/View_Monitordeep_values"
            android:background="@color/light_gray" >

            <TextView
                android:id="@+id/txtDataUnit_layout_subside_pactselect_data_modify_title"
                style="@style/txtRow_common_layout_sudsie_pactselect_data_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="@string/txtValueDataUnit_data_add_or_modify_title" />

            <TextView
                android:id="@+id/txtDataUnit_layout_subside_pactselect_data_modify_content"
                style="@style/txtRow_common_layout_sudsie_pactselect_data_style"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_toRightOf="@id/txtDataUnit_layout_subside_pactselect_data_modify_title"
                android:text="@string/txtValueDataUnit_data_add_or_modify_content" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rlLine_layout_subside_pactselect_data_modify_supervise_date"
            android:layout_width="match_parent"
            android:layout_height="@dimen/rlHeight_layout_subside_pactselect_data_look"
            android:layout_below="@id/rlValueDataUnit_title_layout_subside_pactselect_data_modify"
            android:background="@drawable/common_bgcolor_title" >
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rlSupervise_title_layout_subside_pactselect_data_modify_date"
            android:layout_width="match_parent"
            android:layout_height="@dimen/rlHeight_layout_subside_pactselect_data_row"
            android:layout_below="@id/rlLine_layout_subside_pactselect_data_modify_supervise_date"
            android:background="@color/light_gray" >

            <TextView
                android:id="@+id/txtSuperviseDate_ayout_subside_pactselect_data_modify_date_title"
                style="@style/txtRow_common_layout_sudsie_pactselect_data_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="@string/txtValueSuperviseDate_data_add_or_modify_title" />

            <EditText
                android:id="@+id/editSuperviseDate_ayout_subside_pactselect_data_modify_date_content"
                android:layout_width="match_parent"
                android:layout_height="@dimen/common_EditViewHeight"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="50dp"
                android:layout_toRightOf="@id/txtSuperviseDate_ayout_subside_pactselect_data_modify_date_title"
                android:background="@color/light_gray"
                android:digits="0123456789.-/"
                android:windowSoftInputMode="adjustUnspecified|stateHidden" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="20dp"
                android:focusable="false"
                android:src="@drawable/icon_right_arrows_normal" />
        </RelativeLayout>

        <View
            android:id="@+id/View_data_modify_date_title"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_below="@id/rlSupervise_title_layout_subside_pactselect_data_modify_date"
            android:background="@color/gray" />

        <RelativeLayout
            android:id="@+id/rlUploading_layout_subside_pactselect_data_modify_user"
            android:layout_width="match_parent"
            android:layout_height="@dimen/rlHeight_layout_subside_pactselect_data_row"
            android:layout_below="@id/View_data_modify_date_title"
            android:background="@drawable/selector_common_row" >

            <TextView
                android:id="@+id/txtUploadingUser_layout_subside_pactselect_data_modify_title"
                style="@style/txtRow_common_layout_sudsie_pactselect_data_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="@string/txtValueUploadingUser_data_add_or_modify_title" />

            <TextView
                android:id="@+id/txtUploadingUser_layout_subside_pactselect_data_modifyr_content"
                style="@style/txtRow_common_layout_sudsie_pactselect_data_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:layout_toRightOf="@id/txtUploadingUser_layout_subside_pactselect_data_modify_title"
                android:text="@string/txtValueUploadingUser_data_add_or_modify_content" />
        </RelativeLayout>

        <View
            android:id="@+id/View_data_modify_user"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_below="@id/rlUploading_layout_subside_pactselect_data_modify_user"
            android:background="@color/gray" />

        <Button
            android:id="@+id/butSave_layout_subside_pactselect_data_modify"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_below="@id/View_data_modify_user"
            android:layout_marginLeft="80dp"
            android:layout_marginRight="80dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/selector_common_button"
            android:text="@string/butSave"
            android:textColor="@color/white" />
    </RelativeLayout>

</RelativeLayout>