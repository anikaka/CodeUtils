<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="p02_login_save_pwd_style" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/login_check_selector</item>
    </style>
    
     <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>
     
     <style name="myCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/p13_approve_pop_checkbox_selector</item>
    </style>
    
    <style name="mSettingCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/main_user_seting_check_selector</item>
    </style>
   
    <style name="dialog_anim_bottom_top">
        <item name="android:windowEnterAnimation">@anim/dialog_bottom_to_top</item>
        <item name="android:windowExitAnimation">@anim/dialog_top_to_bottom</item>
    </style> 
    
    <style name="appear_top_right_in_out">
        <item name="android:windowEnterAnimation">@anim/appear_top_right_in</item>
        <item name="android:windowExitAnimation">@anim/appear_top_right_out</item>
    </style> 
    
    
     <style name="wrap2ParentMleft10Theme">
        <item name="android:layout_width">wrap_content</item>
   		<item name="android:layout_height">wrap_content</item>
   		<item name="android:layout_marginLeft">10dp</item>
     </style>
     
     <style name="titleBackgroundTheme" >
   		<item name="android:layout_width">match_parent</item>
   		<item name="android:layout_height">@dimen/common_title_h</item>
   		<item name="android:background">@color/common_color</item>
     </style>
     
     <style name="titleCotentTextViewStyle">
        <item name="android:layout_width">wrap_content</item>
   		<item name="android:layout_height">wrap_content</item>
   		<item name="android:textColor">@color/white</item>
   		<item name="android:textSize">@dimen/common_title_textsize</item>
   		<item name="android:layout_centerInParent">true</item>
         
     </style>
     
     
       <style name="titleMidImageTheme" >
   		<item name="android:layout_width">wrap_content</item>
   		<item name="android:layout_height">wrap_content</item>
   		<item name="android:layout_centerInParent">true</item>
     </style>
    
     
     <style name="titleHomeBtnTheme" >
   		<item name="android:layout_width">wrap_content</item>
   		<item name="android:layout_height">wrap_content</item>
   		<item name="android:layout_marginRight">10dp</item>
   		<item name="android:layout_centerVertical">true</item>
   		<item name="android:background">@drawable/main_title_home_btn_selector</item>
     </style>
     <style name="targetCenterVeticalTheme" >
   		<item name="android:layout_width">0dp</item>
   		<item name="android:layout_height">0dp</item>
   		<item name="android:layout_centerVertical">true</item>
     </style>
     <style name="targetCenterHorizontalTheme" >
   		<item name="android:layout_width">0dp</item>
   		<item name="android:layout_height">0dp</item>
   		<item name="android:layout_centerHorizontal">true</item>
     </style>
     <style name="targetAlignParentRightTheme" >
   		<item name="android:layout_width">0dp</item>
   		<item name="android:layout_height">0dp</item>
   		<item name="android:layout_alignParentRight">true</item>
     </style>
     
    <style name="listViewTheme" >
   		<item name="android:layout_width">match_parent</item>
   		<item name="android:layout_height">match_parent</item>
   		<item name="android:divider">@color/gray</item>
   		<item name="android:dividerHeight">0.1dp</item>
   		<item name="android:cacheColorHint">@null</item>
   		<item name="android:layout_margin">5dp</item>
   		<item name="android:background">@color/white</item>
     </style>
     <style name="p00SpinnerTheme" >
   		<item name="android:layout_width">match_parent</item>
   		<item name="android:layout_height">wrap_content</item>
   		<item name="android:id">@+id/spinner_id</item>
   		<item name="android:paddingLeft">8dp</item>
   		<item name="android:paddingRight">10dp</item>
   		<item name="android:singleLine">true</item>
   		<item name="android:textColor">@color/black</item>
   		<item name="android:drawableRight">@drawable/p00_down_arrow</item>
   		<item name="android:gravity">center_vertical</item>
     </style>
    
     <style  name="tableCell">
         <item name="android:layout_margin">0.5dp</item>
         <item name="android:gravity">center</item>
         <item name="android:background">@color/white</item>
   		<item name="android:textColor">@color/black</item>
   		<item name="android:textSize">@dimen/common_general_font_size</item>
         </style>
</resources>
