<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/civil_portal_row_textview_height"
    android:orientation="horizontal" >

    <!--
         <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/setbar"
        android:orientation="horizontal" >
    -->

    <TextView
        android:id="@+id/ele_rc_checkresultTV1"
        style="@style/Civil_PorTheme"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/setbar"
        android:textSize="@dimen/common_small_font_size" />

    <TextView
        android:id="@+id/ele_rc_checkresultTV2"
        style="@style/Civil_PorTheme"
        android:layout_width="0dp"
       android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/setbar"
        android:textSize="@dimen/common_small_font_size" />

    <TextView
        android:id="@+id/ele_rc_checkresultTV3"
        style="@style/Civil_PorTheme"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:background="@drawable/setbar"
        android:textSize="@dimen/common_small_font_size" />

    <TextView
        android:id="@+id/ele_rc_checkresultTV4"
        style="@style/Civil_PorTheme"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/setbar"
        android:textSize="@dimen/common_small_font_size" />

    <TextView
        android:id="@+id/ele_rc_checkresultTV5"
        style="@style/Civil_PorTheme"
        android:layout_width="0dp"
       android:layout_height="match_parent"
        android:layout_weight="2"
        android:background="@drawable/setbar"
        android:textSize="@dimen/common_small_font_size" />

    <TextView
        android:id="@+id/ele_rc_checkresultTV6"
        style="@style/Civil_PorTheme"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2.5"
        android:background="@drawable/setbar"
        android:textSize="@dimen/common_small_font_size" />

    <TextView
        android:id="@+id/ele_rc_checkresultTV7"
        style="@style/Civil_PorTheme"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/setbar"
        android:textSize="@dimen/common_small_font_size" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/setbar"
        android:orientation="horizontal"
        android:gravity="center_horizontal" 
        >

        <Button
            android:id="@+id/Civil_Por_CheckResultBtn"
            android:layout_width="@dimen/civil_portal_row_btn_width"
            android:layout_height="@dimen/civil_portal_row_btn_height"
            android:layout_gravity="center_vertical"
            android:background="@drawable/delp"
            android:focusable="false"
            android:textAppearance="?android:attr/textAppearanceMedium" />
    </LinearLayout>
    <!-- </LinearLayout> -->

</LinearLayout>